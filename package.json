{
  "name": "exgps",
  "version": "1.0.0",
  "description": "Tool to extract GPS coordinates from photos",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbreslin424/exgps"
  },
  "main": "exgps.js",
  "bin": {
    "exgps": "bin/cli.js"
  },
  "scripts": {
    "start": "node exgps.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls.js && rm -rf ./coverage"
  },
  "author": "Dan Breslin",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.3",
    "jest": "^24.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4"
  },
  "dependencies": {
    "exif": "^0.6.0",
    "handlebars": "^4.1.0",
    "minimist": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "exgps.js",
      "modules/**/*.js",
      "!**/index.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/testSetup.js"
    ]
  }
}
