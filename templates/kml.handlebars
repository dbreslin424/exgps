<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  {{#each placemarks}}
  <Placemark>
    <name>{{this.name}}</name>
    {{#if this.timestamp}}
    <TimeStamp>
      <when>{{this.timestamp}}</when>
    </TimeStamp>
    {{/if}}
    <Point>
      <coordinates>{{this.longitude}},{{this.latitude}},{{this.altitude}}</coordinates>
    </Point>
  </Placemark>
  {{/each}}
</kml>